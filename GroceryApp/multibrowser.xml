<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Multi Browser Test" parallel="tests">
	<test thread-count="5" name="TestChrome">
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="testScripts.loginTestCasrPageFact" />
			<class name="testScripts.manageNewsPageTest" />
			<class name="testScripts.manageFooterText" />
			<class name="testScripts.ManageContactTest" />
