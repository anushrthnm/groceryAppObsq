		</classes>
	</test> <!--
	Test -->
	<test thread-count="5" name="TestFirefox">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="testScripts.loginTestCasrPageFact" />
			<class name="testScripts.manageNewsPageTest" />
			<class name="testScripts.manageFooterText" />
			<class name="testScripts.ManageContactTest" />
			<class name="testScripts.ManageCategoryTest" />
			<class name="testScripts.LogoutTest" />
		</classes>
	</test> <!--
	Test -->
</suite> <!--
